fn main() {
    let a: u8 = 255;
    let b = a.checked_add(20);
    println!("b:{:?}, b:{:#?}", b, b);

    let s1 = String::from("hello");
    let s2 = s1.clone();
    println!("s1 = {}, s2 = {}", s1, s2);

    let a = [1, 2, 3, 4, 5];
    let slice = &a[1..3];
    assert_eq!(slice, &[2, 3]);
    println!("{}", slice== &[2, 3]);
}

